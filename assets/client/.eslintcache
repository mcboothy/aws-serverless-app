port Card from '../UI/Card/Card';\r\nimport classes from './Login.module.css';\r\nimport Button from '../UI/Button/Button';\r\nimport ChatContext from '../../store/chat-context';\r\n\r\nconst stateReducer = (state, action) => {\r\n  let newState = {...state}\r\n  switch (action.type){\r\n    case \"USERNAME_CHANGED\":\r\n      newState.username = action.val;\r\n    break;\r\n  }\r\n\r\n  newState.formIsValid = newState.username.trim().length > 4\r\n  return newState\r\n};\r\n\r\nconst Login = (props) => {\r\n  const ctx = useContext(ChatContext);\r\n\r\n  const [state, dispatchState] = useReducer(stateReducer, {\r\n    username: '',\r\n    formIsValid: false,\r\n  });\r\n\r\n  const userChangeHandler = (event) => {\r\n    dispatchState({type: 'USERNAME_CHANGED', val: event.target.value})\r\n  };\r\n\r\n  const validateUserHandler = () => {\r\n    dispatchState({type: 'USERNAME_BLUR'})\r\n  };\r\n\r\n  const submitHandler = (event) => {\r\n    event.preventDefault();\r\n    ctx.onConnect(state.username);\r\n  };\r\n\r\n  return (\r\n    <Card className={classes.login}>\r\n      <form onSubmit={submitHandler}>\r\n        <div\r\n          className={`${classes.control} ${\r\n            state.formIsValid === false ? classes.invalid : ''\r\n          }`}\r\n        >\r\n          <label htmlFor=\"user\">Name</label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"user\"\r\n            value={state.username}\r\n            onChange={userChangeHandler}\r\n            onBlur={validateUserHandler}\r\n          />\r\n        </div>\r\n        <div className={classes.actions}>\r\n          <Button type=\"submit\" className={classes.btn} disabled={!state.formIsValid}>\r\n            Enter\r\n          </Button>\r\n        </div>\r\n      </form>\r\n    </Card>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","/home/rbooth/Work/software_forum/assets/client/src/components/Home/Home.js",["51"],"import React, {useState, useContext, useRef } from 'react';\r\n\r\nimport Card from '../UI/Card/Card';\r\nimport Button from '../UI/Button/Button';\r\nimport classes from './Home.module.css';\r\nimport ChatContext from '../../store/chat-context';\r\n\r\nconst Home = (props) => {\r\n  const ctx = useContext(ChatContext);\r\n  const inputElement = useRef();\r\n\r\n  const submitHandler = (event) => {\r\n    event.preventDefault();\r\n    ctx.sendMessage(inputElement.value);\r\n  };\r\n\r\n  return (\r\n    <React.Fragment>\r\n    <Card className={classes.home}>\r\n      <form onSubmit={submitHandler}>\r\n        <div\r\n          className={classes.control}\r\n        >\r\n          <label htmlFor=\"user\">Message</label>\r\n          <input\r\n            type=\"text\"\r\n            id=\"user\"\r\n            ref={inputElement}\r\n          />\r\n\r\n          <Button type=\"submit\" className={classes.btn}>\r\n            Send\r\n          </Button>\r\n        </div>\r\n      </form>\r\n    </Card>\r\n    <ul>\r\n      {ctx.messages.map(message => {\r\n        return <Card>\r\n          <div style={{position: 'relative', width: '100%', padding: '1rem'}}>\r\n            <div style={{margin: '0.35rem'}}>\r\n              {message.user}\r\n            </div>\r\n            <div style={{margin: '0.35rem'}}>\r\n              {message.message}\r\n            </div>            \r\n          </div>          \r\n        </Card>\r\n      })}\r\n    </ul>\r\n    </React.Fragment>\r\n\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","/home/rbooth/Work/software_forum/assets/client/src/components/MainHeader/MainHeader.js",[],"/home/rbooth/Work/software_forum/assets/client/src/components/MainHeader/Navigation.js",[],"/home/rbooth/Work/software_forum/assets/client/src/components/UI/Card/Card.js",[],"/home/rbooth/Work/software_forum/assets/client/src/components/UI/Button/Button.js",[],"/home/rbooth/Work/software_forum/assets/client/src/store/chat-context.js",[],{"ruleId":"52","severity":1,"message":"53","line":1,"column":17,"nodeType":"54","messag